pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo "I am in build stage" 
            }
        }
        stage('Test') { 
            steps {
                echo "I am in test stage"
            }
        }
        stage('Deploy') { 
            steps {
                echo "I am in deploy stage"
            }
        }
        stage('Archived') { 
            steps {
                echo "I am in archived stage"
        }
      }
     }
   }
